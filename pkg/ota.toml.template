[auth]
server = "${OTA_AUTH_URL}"
client_id = "${OTA_AUTH_CLIENT_ID}"
secret = "${OTA_AUTH_SECRET}"
credentials_file = "/opt/ats/credentials.toml"

[device]
uuid = "${OTA_CLIENT_UUID}"
vin = "${OTA_CLIENT_VIN}"

[gateway]
console = ${OTA_CONSOLE}
http = ${OTA_HTTP}
websocket = ${OTA_WEBSOCKET}

[ota]
server = "${OTA_CORE_URL}"
polling_interval = 10
packages_dir = "/tmp/"
package_manager = "${PACKAGE_MANAGER}"
